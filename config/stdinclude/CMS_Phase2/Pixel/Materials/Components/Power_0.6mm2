// 0.6 mm^2 power wires
// Serial power: the power lines do not accumulate
Component {
  componentName "Power wires"
  service true
  scaleOnSensor 0
  // Two wires
  Element {
    elementName Cu
    quantity 0.68
    unit g/m
  }
  // Two wires
  Element {
    elementName PE
    quantity 0.54
    unit g/m
  }
  // Two wires 0.6 mm^2
  Element {
    elementName Al
    quantity 3.24
    unit g/m
  }
}

