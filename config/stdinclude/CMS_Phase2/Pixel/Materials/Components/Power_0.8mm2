// 0.8 mm^2 power wires
// Serial power: the power lines do not accumulate
Component {
  componentName "Power wires"
  service true
  scaleOnSensor 0
  // Two wires
  Element {
    elementName Cu
    quantity 0.78
    unit g/m
  }
  // Two wires
  Element {
    elementName PE
    quantity 0.62
    unit g/m
  }
  // Two wires 0.8 mm^2
  Element {
    elementName Al
    quantity 4.32
    unit g/m
  }
}
